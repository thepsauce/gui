main = function {
	local l = float 2.3e-3
	print("float 2.3e-3=", l, "\n")
	local l = .23e2
	print(".23e2=", l, "\n")
	local l = int 33
	print("int 33=", l, "\n")
	local l = 2.3e3
	print("2.3e3=", l, "\n")
	local l = 33
	print("33=", l, "\n")
	local l = int 33e2
	print("int 33e2=", l, "\n")
	local l = 43e3
	print("43e3=", l, "\n")
	local l = float 2.2134e1
	print("2.2134e1=", l, "\n")
	local l = 234e-1
	print("234e-1=", l, "\n")
	return 0
}

Button:
	.font = int 0
	.fontSize = int 16
	.init = function {
		font = CreateFont("FiraCode-Regular.ttf", fontSize)
	}
	.draw = function {
		SetFont(font)
		DrawText("Hello there\nWhat is this?", 30, 30)
	}
	.event = function event e {
		if equals(GetType(e), const EVENT_MOUSEWHEEL) {
			fontSize = sum(fontSize, GetWheel(e))
			if equals(fontSize, 0) {
				fontSize = 1
			}
			font = CreateFont("FiraCode-Regular.ttf", fontSize)
		}
	}
