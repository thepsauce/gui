Button:
	.background = color 0x60609f
	.away = color 0xff0000
	.shadow = color 0xff0000
	.outline = color 0x5b1200
	.text = color 0x00ff00
	.press = color black
	.hover = color black
	.circular = bool true
	.font = font default
	.func = function int x, int y {
		trigger happy
		set a = bool true
		event keydown A
	}
	.s = function int x, int y {
		if equals(x, int 3) {
			set x = int 4
		}
		return sum(x, y, x, y)
	}
	.a = int 33
	.b = int 44
	.c = sum(a, b)
	.f1 = float 23.1
	.f2 = float 1.3
	.fs = sum(f1, f2)
	.fi = sum(a, b, f1, f2, sum(a, b))
	.g = s(int 12, int 55)
	.h = s(int 3, int 12)
